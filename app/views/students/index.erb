<body class="home-page-bkgd">
  <div class="home-page-container">
    <div class="text-center">
      <h1>List All Students Created, <%= current_user.username %></h1>
        <% @students.each do |s| %>
        <h4><a href="/students/<%= s.id %>"><%= s.name%>, <%= s.age %></a></h4>
        <% end %>
        <br><br>
        <h5>Add a New Activity Here</h5>
        <form method="POST" action="/activity">
        <label><strong>Title: *</strong></label><br>
        <input type="text" name="Title" required><br>
        <label><strong>Supplies: *</strong></label><br>
        <input type="text/" name="password"><br>
        <br>
        <input class="btn btn-info" type="submit" value="Log In">
        </form>
    </div>
  </div>
</body>